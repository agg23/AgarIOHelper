{
    "name": "AgarIOHelper",
    "version": "0.1",
    "manifest_version": 2,
	"background": {
    	"scripts": ["jquery.min.js", "background.js"],
    	"persistent": true
	},
	"permissions": ["tabs", "webRequest", "webRequestBlocking", "*://*.agar.io/*"],
	"browser_action": {
        "default_title": "Apply"
        //"default_popup": "popup.html"
    },
    "web_accessible_resources": [
    	"bot.js",
    	"css.css"
    ]
}